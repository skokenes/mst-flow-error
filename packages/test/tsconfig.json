{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom", "es2016", "es2017.object"],
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "include": ["src"]
}
